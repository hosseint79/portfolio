{
  "name": "remix-template-remix",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "start": "remix-serve build",
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -o app/styles/app.css"
  },
  "dependencies": {
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "@remix-run/serve": "^1.5.1",
    "3d-react-carousal": "^5.0.0",
    "concurrently": "^7.2.1",
    "nodemailer": "^6.7.7",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "react-pretty-carousel": "^1.0.19",
    "react-spring": "^9.4.5",
    "react-spring-3d-carousel": "^1.2.1",
    "react-tsparticles": "^2.0.6",
    "remix-utils": "^3.3.0",
    "tiny-invariant": "^1.2.0",
    "tsparticles": "^2.0.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@types/react": "^17.0.45",
    "@types/react-copy-to-clipboard": "^5.0.3",
    "@types/react-dom": "^17.0.17",
    "@types/uuid": "^8.3.4",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.15.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  }
}
