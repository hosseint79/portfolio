{
  "name": "remix-template-remix",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "start": "remix-serve build",
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -o app/styles/app.css",
    "prepare": "husky install",
    "format": "npx prettier --write .",
    "lint": "eslint ."
  },
  "dependencies": {
    "@prisma/client": "^4.1.1",
    "@remix-run/node": "^1.6.5",
    "@remix-run/react": "^1.6.5",
    "@remix-run/serve": "^1.6.5",
    "concurrently": "^7.2.1",
    "framer-motion": "^6.4.3",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.6.0",
    "marked": "^4.0.18",
    "nodemailer": "^6.7.7",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "react-intersection-observer": "^9.3.5",
    "react-spring": "^9.4.5",
    "react-spring-3d-carousel": "^1.2.1",
    "react-toastify": "^9.0.5",
    "react-tsparticles": "^2.0.6",
    "remark-mdx-code-meta": "^2.0.0",
    "remix-utils": "^3.3.0",
    "tiny-invariant": "^1.2.0",
    "tsparticles": "^2.0.6",
    "typewriter-effect": "^2.19.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.5",
    "@remix-run/eslint-config": "^1.6.5",
    "@types/highlight.js": "^10.1.0",
    "@types/marked": "^4.0.3",
    "@types/react": "^17.0.45",
    "@types/react-copy-to-clipboard": "^5.0.3",
    "@types/react-dom": "^17.0.17",
    "@types/react-syntax-highlighter": "^15.5.3",
    "@types/uuid": "^8.3.4",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.15.0",
    "husky": "^8.0.0",
    "postcss": "^8.4.14",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  }
}
